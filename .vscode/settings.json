{
    "simplicity_studio_dir": "/Applications/SimplicityStudio.app/Contents/Eclipse", // verify this path
    "simplicty_studio_gdbserver": "JLinkGDBServer",
    "editor.formatOnSave": true, // optional
    "C_Cpp.default.configurationProvider": "ms-vscode.makefile-tools",
    "makefile.configurations": [
        {
            "name": "Parallel Build",
            "makeArgs": [
                "-j8"
            ]
        }
    ],
    "makefile.extensionLog": "log",
    "makefile.configurationCachePath": ".makefile/configurationCache.log",
    "makefile.extensionOutputFolder": "./.makefile",
    "makefile.makefilePath": "./eoc_demo/eoc_demo.Makefile",
    "makefile.makeDirectory": "./eoc_demo/",
    "makefile.additionalCompilerNames": [
        "arm-none-eabi-gcc"
    ],
    "makefile.phonyOnlyTargets": true,
    "makefile.launchConfigurations": [
        {
            "cwd": "${workspaceFolder}/eoc_demo/build/debug",
            "binaryPath": "${workspaceFolder}/eoc_demo/build/debug/eoc_demo.out",
            "binaryArgs": []
        }
    ],
    "cortex-debug.variableUseNaturalFormat": false,
}